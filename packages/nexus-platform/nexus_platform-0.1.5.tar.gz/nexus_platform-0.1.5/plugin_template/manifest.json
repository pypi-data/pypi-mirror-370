{
  "name": "template_plugin",
  "version": "1.0.0",
  "description": "A template plugin demonstrating best practices for Nexus Framework plugin development",
  "author": "Your Name",
  "category": "example",
  "dependencies": {},
  "permissions": [
    "database.read",
    "database.write",
    "events.publish",
    "events.subscribe",
    "api.register_routes"
  ],
  "entry_point": "create_plugin",
  "main": "plugin.py",
  "config": {
    "enabled": true,
    "max_items": 1000,
    "cache_ttl": 300,
    "enable_notifications": true,
    "api_rate_limit": 100,
    "custom_setting": "default_value",
    "require_database": false
  }
}
