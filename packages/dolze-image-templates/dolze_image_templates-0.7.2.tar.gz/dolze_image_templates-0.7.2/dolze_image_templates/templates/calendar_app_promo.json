{
  "name": "calendar_app_promo",
  "description": "Clean promotional template featuring a calendar app interface",
  "size": { "width": 2560, "height": 2560 },
  "use_base_image": false,
  "components": [
    {
      "type": "rectangle",
      "position": { "x": 0, "y": 0 },
      "size": { "width": 2560, "height": 2560 },
      "fill_color": "${theme_color}",
      "outline_color": null
    },
    {
      "type": "text",
      "text": "${heading}",
      "position": { "x": 142, "y": 190 },
      "font_size": 227,
      "color": [52, 64, 84, 1],
      "font_path": "Outfit-Regular",
      "alignment": "left",
      "max_width": 1422
    },
    {
      "type": "image",
      "image_url": "${logo_url}",
      "position": { "x": 142, "y": 1777 },
      "size": { "width": 285, "height": 285 },
      "circle_crop": true
    },
    {
      "type": "image",
      "image_url": "${image_url}",
      "position": { "x": 947, "y": 853 },
      "size": { "width": 1446, "height": 3006 },
      "border_radius": 47
    },
    {
      "type": "text",
      "text": "${website_url}",
      "position": { "x": 142, "y": 2157 },
      "font_size": 76,
      "color": [52, 64, 84, 1],
      "font_path": "Outfit-Regular",
      "alignment": "left",
      "max_width": 120
    }
  ]
}
