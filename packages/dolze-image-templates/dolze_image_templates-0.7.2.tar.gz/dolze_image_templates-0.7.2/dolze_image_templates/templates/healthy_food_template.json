{
    "name": "healthy_food_template",
    "description": "Bright green healthy food promotion template with salad image and pricing.",
    "size": {
      "width": 1080,
      "height": 1080
    },
    "use_base_image": false,
    "components": [

        {
            "type": "image",
            "image_url": "${background_url}",
            "position": {
              "x": 0,
              "y": 0
            },
            "size": {
              "width": 1080,
              "height": 1080
            },
            "image_fit": "cover"
          },
      {
        "type": "image",
        "image_url": "${salad_image_url}",
        "position": { "x": 220, "y": 105 },
        "size": { "width": 800, "height": 800 },
        "image_fit": "contain"
      },
      {
        "type": "image",
        "image_url": "${arrow_url}",
        "position": { "x": 160, "y": 310 },
        "size": { "width": 125, "height": 125 },
        "image_fit": "contain"
      },
      {
        "type": "text",
        "text": "Fresh & Tasty",
        "position": { "x": 120, "y": 50 },
        "font_size": 32,
        "color": "#FFFFFF",
        "font_path": "HappyMonkey-Regular",
        "alignment": "left",
        "rotate": 25
      },
      {
        "type": "text",
        "text": "${headline}",
        "position": { "x": 70, "y": 790 },
        "font_size": 64,
        "color": "#FFFFFF",
        "font_path": "GreatVibes-Regular",
        "alignment": "left"
      },
      {
        "type": "text",
        "text": "${subtitle}",
        "position": { "x": 70, "y": 860 },
        "font_size": 72,
        "color": "#FFFFFF",
        "font_path": "Poppins-Bold",
        "alignment": "left"
      },
      {
        "type": "rectangle",
        "position": { "x": 830, "y": 800 },
        "size": { "width": 170, "height": 140 },
        "fill_color": "#FFFFFF",
        "border_radius": 60
      },
      {
        "type": "text",
        "text": "${price}",
        "position": { "x": 860, "y": 810 },
        "font_size": 64,
        "color": "#2F7C1A",
        "font_path": "Poppins-Bold",
        "alignment": "left"
      },
      {
        "type": "text",
        "text": "${price_note}",
        "position": { "x": 870, "y": 880 },
        "font_size": 28,
        "color": "#2F7C1A",
        "font_path": "Poppins-Medium",
        "alignment": "left"
      },
      {
        "type": "rectangle",
        "position": { "x": 0, "y": 1000 },
        "size": { "width": 1080, "height": 80 },
        "fill_color": "#FFFFFF",
        "border_radius": 0
      },
      {
        "type": "text",
        "text": "${footer_text_left}",
        "position": { "x": 70, "y": 1020 },
        "font_size": 28,
        "color": "#2F7C1A",
        "font_path": "Poppins-SemiBold",
        "alignment": "left"
      },
    
      {
        "type": "text",
        "text": "FOR FREE DELIVERY: ${footer_text_right}",
        "position": { "x": 600, "y": 1020 },
        "font_size": 24,
        "color": "#2F7C1A",
        "font_path": "Poppins-Medium",
        "alignment": "left"
      },
      {
        "type": "image",
        "image_url": "${logo_url}",
        "position": { "x": 70, "y": 70 },
        "size": { "width": 260, "height": 80 },
        "image_fit": "contain"
      }
    ]
  }
  