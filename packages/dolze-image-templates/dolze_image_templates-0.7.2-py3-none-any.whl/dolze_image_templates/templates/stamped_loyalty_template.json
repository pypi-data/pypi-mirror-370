{
    "name": "stamped_loyalty_template",
    "description": "Stamped Loyalty style template with purple background, central image, CTA, and people tags",
    "size": {
      "width": 540,
      "height": 1080
    },
    "use_base_image": false,
    "components": [
      {
        "type": "rectangle",
        "position": {
          "x": 0,
          "y": 0
        },
        "size": {
          "width": 540,
          "height": 1080
        },
        "fill_color": "#9A3DFE",
        "border_radius": 0
      },
      {
        "type": "image",
        "image_url": "${logo_url}",
        "position": {
          "x": 160,
          "y": 48
        },
        "size": {
          "width": 200,
          "height": 60
        },
        "image_fit": "contain"
      },
      {
        "type": "text",
        "text": "${headline}",
        "position": {
          "x": 40,
          "y": 135
        },
        "font_size": 48,
        "color": "#FFFFFF",
        "font_path": "Poppins-Bold",
        "alignment": "center",
        "max_width": 460,
        "line_height": 1.3
      },
      {
        "type": "rectangle",
        "position": {
          "x": 150,
          "y": 422
        },
        "size": {
          "width": 240,
          "height": 60
        },
        "fill_color": "#1D1D2C",
        "border_radius": 16
      },
      {
        "type": "text",
        "text": "${cta_text}",
        "position": {
          "x": 170,
          "y": 435
        },
        "font_size": 24,
        "color": "#FFFFFF",
        "font_path": "Poppins-Medium",
        "alignment": "center",
        "max_width": 200
      },
      {
        "type": "image",
        "image_url": "https://i.postimg.cc/8kXNxS2q/Frame-1244832598.png",
        "position": {
          "x": 0,
          "y": 560
        },
        "size": {
          "width": 540,
          "height": 540
        },
        "image_fit": "cover"
      }
    ]
  }
  