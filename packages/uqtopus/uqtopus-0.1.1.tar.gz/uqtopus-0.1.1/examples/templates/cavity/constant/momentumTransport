FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      momentumTransport;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType  RAS;

RAS
{
    model           kEpsilon;

    turbulence      on;

    printCoeffs     on;

    kEpsilonCoeffs
    {
        Cmu        {{Cmu}};
        C1         {{C1}};
        C2         {{C2}};
        sigmaK     {{sigmaK}};
        sigmaEps   {{sigmaEps}};
    }
}

