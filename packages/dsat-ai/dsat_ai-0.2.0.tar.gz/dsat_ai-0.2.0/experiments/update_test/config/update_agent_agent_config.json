{
  "update_agent": {
    "agent_name": "update_agent",
    "model_provider": "google",
    "model_family": "claude",
    "model_version": "claude-3-5-haiku-latest",
    "prompt": "updated_prompt:v3",
    "model_parameters": {
      "temperature": 0.7,
      "max_tokens": 4096
    },
    "provider_auth": {
      "api_key": "your-api-key-here"
    },
    "custom_configs": {
      "retry_attempts": 3,
      "rate_limit_rpm": 60,
      "logging_enabled": true,
      "cache_responses": false
    },
    "tools": []
  }
}