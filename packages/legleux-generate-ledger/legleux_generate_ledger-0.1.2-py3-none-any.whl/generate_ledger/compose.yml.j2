version: "3.9"
services:
  {{ service_name }}:
    image: {{ image }}
    {% if command %}
    command: [{{ command|map('tojson')|join(', ') }}]
    {% endif %}
    {% if healthcheck %}
    healthcheck:
      test: [{{ healthcheck.test|map('tojson')|join(', ') }}]
      interval: {{ healthcheck.interval }}s
      start_period: {{ healthcheck.start_period }}s
    {% endif %}
