[parser]
Value number (\d+)
Value text (\S+)
Value ip (\d+\.\d+\.\d+\.\d+)
Value mask (\d+)

Start
  ^${number} ${text} ${ip} ${mask} -> Record

[output]
original_number => {{ number }}
doubled => {{ multiply(number, 2) }}
text_upper => {{ custom_upper(text) }}
ip_network => {{ format_ip(ip, mask) }}
binary => {{ to_binary(number) }}
calculation => {{ number }} * 5 = {{ multiply(number, 5) }}