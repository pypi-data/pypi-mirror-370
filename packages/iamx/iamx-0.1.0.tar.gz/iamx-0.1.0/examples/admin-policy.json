{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AdminAccess",
      "Effect": "Allow",
      "Action": [
        "iam:*",
        "s3:*",
        "ec2:*",
        "rds:*",
        "lambda:*",
        "cloudformation:*",
        "cloudwatch:*",
        "logs:*",
        "kms:*",
        "secretsmanager:*",
        "ssm:*",
        "sts:*"
      ],
      "Resource": "*"
    },
    {
      "Sid": "CrossAccountAccess",
      "Effect": "Allow",
      "Action": [
        "sts:AssumeRole"
      ],
      "Resource": "arn:aws:iam::*:role/*"
    },
    {
      "Sid": "DataAccess",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject",
        "dynamodb:GetItem",
        "dynamodb:PutItem",
        "dynamodb:DeleteItem"
      ],
      "Resource": "*"
    }
  ]
}
