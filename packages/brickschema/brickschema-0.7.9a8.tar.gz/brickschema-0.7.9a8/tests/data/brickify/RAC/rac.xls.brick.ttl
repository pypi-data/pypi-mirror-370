@prefix bldg: <https://example.com/bldg#> .
@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix site: <https://example.com/site#> .

bldg:X-AHU-1 a brick:AHU,
        brick:Equipment ;
    brick:feeds bldg:VAV-01-01 .

bldg:X-AHU-2 a brick:AHU,
        brick:Equipment ;
    brick:feeds bldg:VAV-02-01 .

site:Site_A a brick:Site ;
    brick:isLocationOf bldg:Building_X .

bldg:101_Meeting_Room_HZ a brick:HVAC_Zone ;
    brick:hasLocation bldg:Floor_1 ;
    brick:hasPart bldg:101_Meeting_Room .

bldg:201_Office_HZ a brick:HVAC_Zone ;
    brick:hasLocation bldg:Floor_2 ;
    brick:hasPart bldg:201_Office .

bldg:Building_X a brick:Building ;
    brick:hasPart bldg:Floor_1,
        bldg:Floor_2 .

bldg:VAV-01-01 a brick:Equipment,
        brick:VAV ;
    brick:feeds bldg:101_Meeting_Room_HZ ;
    brick:hasPoint bldg:VAV-01-01-OS,
        bldg:VAV-01-01-RH,
        bldg:VAV-01-01-RH-HHA,
        bldg:VAV-01-01-RH-LHA,
        bldg:VAV-01-01-RH-SP .

bldg:VAV-01-01-RH-HHA a brick:High_Humidity_Alarm .

bldg:VAV-01-01-RH-LHA a brick:Low_Humidity_Alarms .

bldg:VAV-01-01-RH-SP a brick:Relative_Humidity_Setpoint .

bldg:VAV-02-01 a brick:Equipment,
        brick:VAV ;
    brick:feeds bldg:201_Office_HZ ;
    brick:hasPoint bldg:VAV-02-01-CO2,
        bldg:VAV-02-01-CO2-SP,
        bldg:VAV-02-01-CO2A,
        bldg:VAV-02-01-OS,
        bldg:VAV-02-01-RH,
        bldg:VAV-02-01-RH-HHA,
        bldg:VAV-02-01-RH-LHA,
        bldg:VAV-02-01-RH-SP .

bldg:VAV-02-01-CO2-SP a brick:CO2_Setpoint .

bldg:VAV-02-01-RH-HHA a brick:High_Humidity_Alarm .

bldg:VAV-02-01-RH-LHA a brick:Low_Humidity_Alarms .

bldg:VAV-02-01-RH-SP a brick:Relative_Humidity_Setpoint .

bldg:101_Meeting_Room a brick:Room,
        brick:Space ;
    brick:isLocationOf bldg:VAV-01-01-OS,
        bldg:VAV-01-01-RH .

bldg:201_Office a brick:Space ;
    brick:isLocationOf bldg:VAV-02-01-CO2,
        bldg:VAV-02-01-CO2A,
        bldg:VAV-02-01-OS,
        bldg:VAV-02-01-RH .

bldg:Floor_1 a brick:Floor ;
    brick:hasPart bldg:101_Meeting_Room .

bldg:Floor_2 a brick:Floor ;
    brick:hasPart bldg:201_Office .

bldg:VAV-01-01-OS a brick:Occupancy_Sensor .

bldg:VAV-01-01-RH a brick:Relative_Humidity_Sensor .

bldg:VAV-02-01-CO2 a brick:CO2_Sensor .

bldg:VAV-02-01-CO2A a brick:High_CO2_Alarm .

bldg:VAV-02-01-OS a brick:Occupancy_Sensor .

bldg:VAV-02-01-RH a brick:Relative_Humidity_Sensor .

