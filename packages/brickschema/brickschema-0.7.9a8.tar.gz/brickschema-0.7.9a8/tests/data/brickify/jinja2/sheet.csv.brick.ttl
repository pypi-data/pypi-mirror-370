@prefix bldg: <https://example.com/bldg#> .
@prefix brick: <https://brickschema.org/schema/Brick#> .

bldg:A brick:hasPoint bldg:A_sp_0,
        bldg:A_sp_1,
        bldg:A_sp_2,
        bldg:A_ts_0,
        bldg:A_ts_1,
        bldg:A_ts_2,
        bldg:A_ts_3 .

bldg:A_0 a brick:VAV .

bldg:B a brick:RVAV ;
