        bldg:B_sp_1,
        bldg:B_sp_2,
        bldg:B_ts_0,
        bldg:B_ts_1,
        bldg:B_ts_2,
        bldg:B_ts_3,
        bldg:B_ts_4 .

bldg:B_0 a brick:VAV .

bldg:A_sp_0 a brick:Temperature_Setpoint .

bldg:A_sp_1 a brick:Temperature_Setpoint .

bldg:A_sp_2 a brick:Temperature_Setpoint .

