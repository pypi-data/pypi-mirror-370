        "dark": "/assets/logos/qredence-dark.svg"
    },
    "favicon": "/assets/icons/favicon.png",
    "colors": {
        "primary": "#2563eb",
        "light": "#3b82f6",
        "dark": "#1d4ed8",
        "background": {
            "light": "#ffffff",
            "dark": "#0d1117"
        }
    },
    "theme": "mint",
    "layout": "topnav",
