[analytics]
check_for_updates = true

[grafana]
root_url = {{ grafana_url }}# Grafana url

[log]
mode = console

[paths]
data = /var/lib/grafana/data
logs = /var/log/grafana
plugins = /var/lib/grafana/plugins

[server]
protocol = http
http_addr = 0.0.0.0
http_port = {{ http_port }}# Change this port as needed
enforce_domain = false
domain = {{ domain }}
root_url = %(protocol)s://%(domain)s:%(http_port)s/monitor
serve_from_sub_path = true
enable_gzip = true

[users]
default_theme = light
org_role = Admin
allow_sign_up = false

[auth.anonymous]
enabled = true
org_name = Main Org.
org_role = Viewer
hide_version = true

[security]
admin_user = admin
admin_password = admin