{
  "command": "force_delete_by_uuids",
  "summary": "Force delete chunks by UUIDs",
  "description": "Force delete chunks by their UUIDs, bypassing normal deletion checks",
  "parameters": {
    "uuids": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "minItems": 1,
      "description": "List of UUIDs (any strings) to delete"
    }
  },
  "required": ["uuids"],
  "examples": {
    "force_delete_single": {
      "description": "Force delete a single chunk",
      "request": {
        "jsonrpc": "2.0",
        "method": "force_delete_by_uuids",
        "params": {
          "uuids": ["12345678-1234-1234-1234-123456789abc"]
        },
        "id": 1
      }
    },
    "force_delete_multiple": {
      "description": "Force delete multiple chunks",
      "request": {
        "jsonrpc": "2.0",
        "method": "force_delete_by_uuids",
        "params": {
          "uuids": [
            "12345678-1234-1234-1234-123456789abc",
            "87654321-4321-4321-4321-cba987654321"
          ]
        },
        "id": 1
      }
    }
  }
} 