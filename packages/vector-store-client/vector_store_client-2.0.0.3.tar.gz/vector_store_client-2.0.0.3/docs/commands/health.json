{
  "command": "health",
  "summary": "Command that returns information about server health and status",
  "description": "Check the health status of the service and get basic information",
  "parameters": {},
  "required": [],
  "response_schema": {
    "status": {
      "type": "string",
      "description": "Status of the service (ok/error)"
    },
    "model": {
      "type": "string",
      "description": "Current active model"
    },
    "version": {
      "type": "string",
      "description": "Version of the service"
    }
  },
  "examples": {
    "health_check": {
      "description": "Check service health",
      "request": {
        "jsonrpc": "2.0",
        "method": "health",
        "id": 1
      },
      "response": {
        "jsonrpc": "2.0",
        "result": {
          "status": "ok",
          "model": "sentence-transformers/all-MiniLM-L6-v2",
          "version": "1.0.0"
        },
        "id": 1
      }
    }
  }
} 