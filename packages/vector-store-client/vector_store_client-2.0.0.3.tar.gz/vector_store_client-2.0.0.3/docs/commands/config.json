{
  "command": "config",
  "summary": "Command for managing service configuration",
  "description": "Get or set configuration parameters for the service",
  "parameters": {
    "operation": {
      "type": "string",
      "enum": ["get", "set"],
      "default": "get",
      "description": "Operation to perform (get or set)"
    },
    "path": {
      "type": "string",
      "description": "Configuration path in dot notation (e.g. 'server.host')"
    },
    "value": {
      "description": "Value to set (required for 'set' operation)"
    }
  },
  "required": ["operation"],
  "examples": {
    "get_config": {
      "description": "Get configuration value",
      "request": {
        "jsonrpc": "2.0",
        "method": "config",
        "params": {
          "operation": "get",
          "path": "server.host"
        },
        "id": 1
      }
    },
    "set_config": {
      "description": "Set configuration value",
      "request": {
        "jsonrpc": "2.0",
        "method": "config",
        "params": {
          "operation": "set",
          "path": "server.host",
          "value": "localhost"
        },
        "id": 1
      }
    }
  }
} 