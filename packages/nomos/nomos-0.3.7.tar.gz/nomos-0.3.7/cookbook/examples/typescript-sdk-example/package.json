{
  "name": "nomos-typescript-example",
  "version": "1.0.0",
  "description": "Example TypeScript/JavaScript application using Nomos SDK",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "start:js": "node simple-example.js",
    "basic": "tsc && node dist/basic-example.js",
    "advanced": "tsc && node dist/advanced-example.js",
    "interactive": "tsc && node dist/interactive-chat.js"
  },
  "dependencies": {
    "nomos-sdk": "*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
