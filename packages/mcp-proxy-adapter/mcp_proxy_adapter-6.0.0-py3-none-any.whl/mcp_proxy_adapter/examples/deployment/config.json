{
  "server": {
    "host": "0.0.0.0",
    "port": 8000,
    "debug": false,
    "log_level": "info"
  },
  "logging": {
    "level": "INFO",
    "file": "adapter.log",
    "rotation": {
      "type": "size",
      "max_bytes": 10485760,
      "backup_count": 5
    }
  },
  "service": {
    "name": "MCP Proxy Adapter",
    "version": "1.0.0"
  },
  "discovery": {
    "enabled": true,
    "package": "commands"
  },
  "docker": {
    "container_name": "mcp-proxy-adapter",
    "network": "default"
  }
}