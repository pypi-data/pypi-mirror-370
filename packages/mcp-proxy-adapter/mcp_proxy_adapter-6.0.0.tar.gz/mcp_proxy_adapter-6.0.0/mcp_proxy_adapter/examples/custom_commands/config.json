{
  "server": {
    "host": "0.0.0.0",
    "port": 8000,
    "debug": true,
    "log_level": "INFO"
  },
  "logging": {
    "level": "INFO",
    "file": null,
    "log_dir": "./logs",
    "log_file": "mcp_proxy_adapter.log",
    "error_log_file": "mcp_proxy_adapter_error.log",
    "access_log_file": "mcp_proxy_adapter_access.log",
    "max_file_size": "10MB",
    "backup_count": 5,
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "date_format": "%Y-%m-%d %H:%M:%S",
    "console_output": true,
    "file_output": true
  },
  "commands": {
    "auto_discovery": true,
    "commands_directory": "./commands",
    "catalog_directory": "./catalog",
    "plugin_servers": [],
    "auto_install_dependencies": true,
    "discovery_path": "mcp_proxy_adapter.commands"
  },
  "ssl": {
    "enabled": false,
    "mode": "http_only",
    "cert_file": null,
    "key_file": null,
    "ca_cert": null,
    "verify_client": false,
    "client_cert_required": false,
    "cipher_suites": [
      "TLS_AES_256_GCM_SHA384",
      "TLS_CHACHA20_POLY1305_SHA256"
    ],
    "min_tls_version": "1.2",
    "max_tls_version": "1.3",
    "token_auth": {
      "enabled": false,
      "header_name": "Authorization",
      "token_prefix": "Bearer",
      "tokens_file": "tokens.json",
      "token_expiry": 3600,
      "jwt_secret": "",
      "jwt_algorithm": "HS256"
    }
  },
  "roles": {
    "enabled": false,
    "config_file": "schemas/roles_schema.json",
    "default_policy": {
      "deny_by_default": true,
      "require_role_match": true,
      "case_sensitive": false,
      "allow_wildcard": true
    },
    "auto_load": true,
    "validation_enabled": true
  },
  "transport": {
    "type": "http",
    "port": null,
    "ssl": {
      "enabled": false,
      "cert_file": null,
      "key_file": null,
      "ca_cert": null,
      "verify_client": false,
      "client_cert_required": false
    }
  },
  "proxy_registration": {
    "enabled": true,
    "proxy_url": "http://localhost:3004",
    "server_id": "mcp_proxy_adapter_custom",
    "server_name": "Custom Commands MCP Proxy Adapter Server",
    "description": "JSON-RPC API for custom commands example",
    "registration_timeout": 30,
    "retry_attempts": 3,
    "retry_delay": 5,
    "auto_register_on_startup": false,
    "auto_unregister_on_shutdown": true
  },
  "debug": {
    "enabled": false,
    "level": "WARNING"
  },
  "protocols": {
    "enabled": false,
    "allowed_protocols": [
      "http"
    ],
    "http": {
      "enabled": true,
      "port": 8000
    },
    "https": {
      "enabled": false,
      "port": 8443
    },
    "mtls": {
      "enabled": false,
      "port": 9443
    }
  },
  "auth_enabled": false,
  "rate_limit_enabled": false,
  "custom": {
    "description": "HTTP transport server example",
    "server_name": "HTTP Transport MCP Proxy Adapter Server"
  }
}
