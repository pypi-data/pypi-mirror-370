{
  "server": {
    "host": "0.0.0.0",
    "port": 8000,
    "debug": true,
    "log_level": "INFO"
  },
  "logging": {
    "level": "INFO",
    "log_dir": "./logs",
    "console_output": true
  },
  "commands": {
    "auto_discovery": true,
    "discovery_path": "mcp_proxy_adapter.commands"
  },
  "ssl": {
    "enabled": false
  },
  "protocols": {
    "enabled": true,
    "allowed_protocols": ["http"],
    "http": {
      "enabled": true,
      "port": 8000
    },
    "https": {
      "enabled": false,
      "port": 8443
    },
    "mtls": {
      "enabled": false,
      "port": 9443
    }
  },
  "proxy_registration": {
    "enabled": true,
    "proxy_url": "http://localhost:3004",
    "server_id": "mcp_proxy_adapter_http",
    "server_name": "HTTP MCP Proxy Adapter Server",
    "description": "JSON-RPC API for interacting with MCP Proxy (HTTP mode)",
    "registration_timeout": 30,
    "retry_attempts": 3,
    "retry_delay": 5,
    "auto_register_on_startup": true,
    "auto_unregister_on_shutdown": true
  },
  "custom": {
    "description": "HTTP-only server example",
    "server_name": "HTTP MCP Proxy Adapter Server"
  }
} 