{
  "server": {
    "host": "0.0.0.0",
    "port": 8000,
    "debug": true,
    "log_level": "INFO"
  },
  "logging": {
    "level": "INFO",
    "log_dir": "./logs",
    "console_output": true
  },
  "commands": {
    "auto_discovery": true,
    "discovery_path": "mcp_proxy_adapter.commands"
  },
  "ssl": {
    "enabled": false
  },
  "protocols": {
    "enabled": true,
    "allowed_protocols": ["http"],
    "http": {
      "enabled": true,
      "port": 8000
    },
    "https": {
      "enabled": false,
      "port": 8443
    },
    "mtls": {
      "enabled": false,
      "port": 9443
    }
  }
} 