{
  "cSpell.words": [
    "ajob",
    "Checkin",
    "loguru",
    "machdep",
    "Mbit",
    "mbps",
    "pkey",
    "plutil",
    "procs",
    "sessionmaker",
    "testsuites",
    "untar",
    "Untaring",
    "VERIBLE",
    "verilog",
    "workdir"
  ],
  "python.languageServer": "Pylance",
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit"
    }
  },
  "ruff.fixAll": true,
  "ruff.organizeImports": true,
  "makefile.configureOnOpen": false
}
