#: flyer_composer/cli.py:53
#, python-format
msgid "I don't understand your box specification %r"
msgstr "No se comprende la especificación de cuadro %r proporcionada"

#: flyer_composer/cli.py:57
msgid "Offset not allowed in box definition"
msgstr "Desplazamiento no permitido en la definición del cuadro"

#: flyer_composer/cli.py:68
#, python-format
msgid "I don't understand your papersize name %r"
msgstr "No se comprende el nombre de tamaño de papel %r proporcionado"

#: flyer_composer/cli.py:71
#, python-format
msgid "Your papersize name %r is not unique, give more chars"
msgstr "El nombre de tamaño del papel %r no es único, proporciona más caracteres"

#: flyer_composer/cli.py:103
msgid "list available media and distance names"
msgstr "enumerar nombres de soportes y distancias disponibles"

#: flyer_composer/cli.py:105
msgid "show program's version number and exit"
msgstr "mostrar el número de versión del programa y salir"

#: flyer_composer/cli.py:108
msgid "show what would have been done, but do not generate files"
msgstr "mostrar lo que se efectuaría pero sin generar ningún archivo"

#: flyer_composer/cli.py:111
msgid "Define Input"
msgstr "Especificar la entrada"

#: flyer_composer/cli.py:113 flyer_composer/cli.py:116
msgid "INTEGER"
msgstr "INTEGER"

#: flyer_composer/cli.py:114
msgid "front page of flyer (default: first page)"
msgstr "portada del folleto (predeterminado: primera página)"

#: flyer_composer/cli.py:117
msgid "back page of flyer (default: no back page)"
msgstr "contraportada del folleto (predeterminado: sin contraportada)"

#: flyer_composer/cli.py:120
msgid ""
"use the content area defined by the ArtBox (default: use the area defined"
" by the TrimBox)"
msgstr ""
"usar el área del contenido definida por ArtBox (predeterminado: usar el "
"área definida por TrimBox)"

#: flyer_composer/cli.py:124
msgid "Define Output"
msgstr "Especificar el resultado"

#: flyer_composer/cli.py:126 flyer_composer/cli.py:131
msgid "BOX"
msgstr "BOX"

#: flyer_composer/cli.py:127
msgid "specify the finished flyer size (defaults to media size)"
msgstr "especifique el tamaño final del folleto (por defecto es el tamaño medio)"

#: flyer_composer/cli.py:132
#, python-format
msgid "specify the page size of the output media (default: %s)"
msgstr "específica el tamaño de la página del soporte de salida (por defecto: %s)"

#: flyer_composer/cli.py:135
msgid "InputFile"
msgstr "Archivo de entrada"

#: flyer_composer/cli.py:136
msgid "OutputFile"
msgstr "Archivo de salida"

#: flyer_composer/cli.py:155
#, python-format
msgid "The input-file is either corrupt or no PDF at all: %s"
msgstr "El archivo de entrada está dañado o no tiene un PDF: %s"

#: flyer_composer/i18n.py:47
msgid "optional arguments"
msgstr "argumentos opcionales"

#: flyer_composer/gui/__init__.py:57
msgid "A4"
msgstr "A4"

#: flyer_composer/gui/__init__.py:58
msgid "A5 on A4"
msgstr "A5 sobre A4"

#: flyer_composer/gui/__init__.py:59
msgid "A6 on A4"
msgstr "A6 sobre A4"

#: flyer_composer/gui/__init__.py:60
msgid "DIN Lang on A4"
msgstr "DIN Lang sobre A4"

#: flyer_composer/gui/__init__.py:197
msgid "&Open…"
msgstr "&Abrir…"

#: flyer_composer/gui/__init__.py:198
msgid "Open PDF document to use for this page"
msgstr "Abra el documento PDF que se utilizará para esta página"

#: flyer_composer/gui/__init__.py:219
msgid "Remove this page"
msgstr "Quitar esta página"

#: flyer_composer/gui/__init__.py:237
msgid "Select page:"
msgstr "Selecciona la página:"

#: flyer_composer/gui/__init__.py:251
msgid "Open document"
msgstr "Abrir un documento"

#: flyer_composer/gui/__init__.py:253 flyer_composer/gui/__init__.py:416
msgid "PDF document (*.pdf);;All Files (*)"
msgstr "Documento PDF (*.pdf);;Todos los archivos (*)"

#: flyer_composer/gui/__init__.py:314
msgid "&File"
msgstr "&Archivo"

#: flyer_composer/gui/__init__.py:315
msgid "&Exit"
msgstr "&Salir"

#: flyer_composer/gui/__init__.py:330
msgid "Front page"
msgstr "Anverso"

#: flyer_composer/gui/__init__.py:330
msgid "Back page"
msgstr "Reverso"

#: flyer_composer/gui/__init__.py:336
#, python-format
msgid ""
"If you find %(APPLICATION_NAME)s useful, please <a "
"href=\"%(DONATION_URL)s\">donate</a>!"
msgstr ""
"¡Si %(APPLICATION_NAME)s te resulta útil, <a "
"href=\"%(DONATION_URL)s\">done</a>!"

#: flyer_composer/gui/__init__.py:342
msgid "Select target layout:"
msgstr "Seleccione la disposición de destino:"

#: flyer_composer/gui/__init__.py:360
msgid "&Save as…"
msgstr "&Guardar como…"

#: flyer_composer/gui/__init__.py:361
msgid "Save flyer as PDF"
msgstr "Guardar el folleto como un PDF"

#: flyer_composer/gui/__init__.py:369
msgid "&Donate…"
msgstr "&Donar…"

#: flyer_composer/gui/__init__.py:370
msgid "Support development of this project"
msgstr "Apoye el desarrollo de este proyecto"

#: flyer_composer/gui/__init__.py:383
#, python-format
msgid "Failed to load the file %s"
msgstr "No se pudo cargar el archivo %s"

#: flyer_composer/gui/__init__.py:409
msgid "-flyer"
msgstr "-volante"

#: flyer_composer/gui/__init__.py:414
msgid "Save document"
msgstr "Guardar documento"

#: ../../../../../../usr/lib64/python3.10/argparse.py:296
msgid "usage: "
msgstr "uso: "

#: ../../../../../../usr/lib64/python3.10/argparse.py:868
msgid ".__call__() not defined"
msgstr ".__call__() no definido"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1211
#, python-format
msgid "unknown parser %(parser_name)r (choices: %(choices)s)"
msgstr "analizador desconocido %(parser_name)r (opciones: %(choices)s)"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1271
#, python-format
msgid "argument \"-\" with mode %r"
msgstr "argumento \"-\" con el modo %r"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1280
#, python-format
msgid "can't open '%(filename)s': %(error)s"
msgstr "No se puede abrir '%(filename)s': %(error)s"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1489
#, python-format
msgid "cannot merge actions - two groups are named %r"
msgstr "no se pueden fusionar las acciones - dos grupos se llaman %r"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1527
msgid "'required' is an invalid argument for positionals"
msgstr "'requerido' es un argumento inválido para posicionales"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1549
#, python-format
msgid ""
"invalid option string %(option)r: must start with a character "
"%(prefix_chars)r"
msgstr ""
"cadena de opción no válida %(option)r: debe comenzar con un carácter "
"%(prefix_chars)r"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1567
#, python-format
msgid "dest= is required for options like %r"
msgstr "dest= es necesario para las opciones como %r"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1584
#, python-format
msgid "invalid conflict_resolution value: %r"
msgstr "valor no válido para conflict_resolution: %r"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1604
#, python-format
msgid "conflicting option string: %s"
msgid_plural "conflicting option strings: %s"
msgstr[0] "cadena de opción conflictiva: %s"
msgstr[1] "cadenas de opciones conflictivas: %s"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1668
msgid "mutually exclusive arguments must be optional"
msgstr "los argumentos mutuamente excluyentes deben ser opcionales"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1736
msgid "positional arguments"
msgstr "argumentos posicionales"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1737
msgid "options"
msgstr "opciones"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1752
msgid "show this help message and exit"
msgstr "mostrar este mensaje de ayuda y salir"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1783
msgid "cannot have multiple subparser arguments"
msgstr "no es posible tener varios argumentos del subanalizador"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1835
#: ../../../../../../usr/lib64/python3.10/argparse.py:2350
#, python-format
msgid "unrecognized arguments: %s"
msgstr "argumentos no reconocidos: %s"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1936
#, python-format
msgid "not allowed with argument %s"
msgstr "no se permite en conjunto con el argumento %s"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1986
#: ../../../../../../usr/lib64/python3.10/argparse.py:2000
#, python-format
msgid "ignored explicit argument %r"
msgstr "se pasó por alto el argumento explícito %r"

#: ../../../../../../usr/lib64/python3.10/argparse.py:2107
#, python-format
msgid "the following arguments are required: %s"
msgstr "los argumentos siguientes son obligatorios: %s"

#: ../../../../../../usr/lib64/python3.10/argparse.py:2122
#, python-format
msgid "one of the arguments %s is required"
msgstr "se requiere uno de los argumentos %s"

#: ../../../../../../usr/lib64/python3.10/argparse.py:2165
msgid "expected one argument"
msgstr "se esperaba un argumento"

#: ../../../../../../usr/lib64/python3.10/argparse.py:2166
msgid "expected at most one argument"
msgstr "se esperaba un argumento como máximo"

#: ../../../../../../usr/lib64/python3.10/argparse.py:2167
msgid "expected at least one argument"
msgstr "se esperaba por lo menos un argumento"

#: ../../../../../../usr/lib64/python3.10/argparse.py:2171
#, python-format
msgid "expected %s argument"
msgid_plural "expected %s arguments"
msgstr[0] "%s argumento esperado"
msgstr[1] "%s argumentos esperados"

#: ../../../../../../usr/lib64/python3.10/argparse.py:2229
#, python-format
msgid "ambiguous option: %(option)s could match %(matches)s"
msgstr "opción ambigua: %(option)s podría coincidir con %(matches)s"

#: ../../../../../../usr/lib64/python3.10/argparse.py:2293
#, python-format
msgid "unexpected option string: %s"
msgstr "cadena de opción inesperada: %s"

#: ../../../../../../usr/lib64/python3.10/argparse.py:2490
#, python-format
msgid "%r is not callable"
msgstr "%r no se puede invocar"

#: ../../../../../../usr/lib64/python3.10/argparse.py:2507
#, python-format
msgid "invalid %(type)s value: %(value)r"
msgstr "valor %(type)s no válido: %(value)r"

#: ../../../../../../usr/lib64/python3.10/argparse.py:2518
#, python-format
msgid "invalid choice: %(value)r (choose from %(choices)s)"
msgstr "elección no válida: %(value)r (elegir entre %(choices)s)"

#: ../../../../../../usr/lib64/python3.10/argparse.py:2594
#, python-format
msgid "%(prog)s: error: %(message)s\n"
msgstr "%(prog)s: error: %(message)s\n"
