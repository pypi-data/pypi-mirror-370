#: ../../../../../../usr/lib64/python3.10/argparse.py:296
msgid "usage: "
msgstr "användning: "

#: ../../../../../../usr/lib64/python3.10/argparse.py:868
msgid ".__call__() not defined"
msgstr ".__call__() är inte definierad"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1211
#, python-format
msgid "unknown parser %(parser_name)r (choices: %(choices)s)"
msgstr "okänd parser %(parser_name)r (valmöjligheter: %(choices)s)"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1271
#, python-format
msgid "argument \"-\" with mode %r"
msgstr "argument \"-\" med läge %r"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1280
#, python-format
msgid "can't open '%(filename)s': %(error)s"
msgstr "kan inte öppna '%(filename)s': %(error)s"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1489
#, python-format
msgid "cannot merge actions - two groups are named %r"
msgstr "kan inte sammanfoga ageranden(actions) - två grupper kallas %r"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1527
msgid "'required' is an invalid argument for positionals"
msgstr "'required' är en otillåten parameter för ordningsföljdsberoende parametrar"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1549
#, python-format
msgid ""
"invalid option string %(option)r: must start with a character "
"%(prefix_chars)r"
msgstr ""
"felaktig parametersträng %(option)r: måste börja med ett av tecknen "
"%(prefix_chars)r"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1567
#, python-format
msgid "dest= is required for options like %r"
msgstr "dest= krävs för parametrar som %r"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1584
#, python-format
msgid "invalid conflict_resolution value: %r"
msgstr "felaktigt värde för conflict_resolution: %r"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1604
#, python-format
msgid "conflicting option string: %s"
msgid_plural "conflicting option strings: %s"
msgstr[0] "sträng för parameternamn står i konflikt: %s"
msgstr[1] "strängar för parameternamn står i konflikt: %s"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1668
msgid "mutually exclusive arguments must be optional"
msgstr "parametrar som inte tillåts tillsammans måste vara frivilliga"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1736
msgid "positional arguments"
msgstr "ordningsföljdsberoende parametrar"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1737
msgid "options"
msgstr "alternativ"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1752
msgid "show this help message and exit"
msgstr "visa denna hjälp och avsluta"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1783
msgid "cannot have multiple subparser arguments"
msgstr "kan inte ha flera subparser-parametrar"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1835
#: ../../../../../../usr/lib64/python3.10/argparse.py:2350
#, python-format
msgid "unrecognized arguments: %s"
msgstr "ej igenkända parametrar: %s"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1936
#, python-format
msgid "not allowed with argument %s"
msgstr "tillåts ej med parametern/parametrarna %s"

#: ../../../../../../usr/lib64/python3.10/argparse.py:1986
#: ../../../../../../usr/lib64/python3.10/argparse.py:2000
#, python-format
msgid "ignored explicit argument %r"
msgstr "ignorerade uttryckligt givet parametervärde %r"

#: ../../../../../../usr/lib64/python3.10/argparse.py:2107
#, python-format
msgid "the following arguments are required: %s"
msgstr "följande parametrar är obligatoriska: %s"

#: ../../../../../../usr/lib64/python3.10/argparse.py:2122
#, python-format
msgid "one of the arguments %s is required"
msgstr "en av parametrarna %s krävs"

#: ../../../../../../usr/lib64/python3.10/argparse.py:2165
msgid "expected one argument"
msgstr "förväntade sig ett parametervärde"

#: ../../../../../../usr/lib64/python3.10/argparse.py:2166
msgid "expected at most one argument"
msgstr "som mest ett parametervärde förväntades"

#: ../../../../../../usr/lib64/python3.10/argparse.py:2167
msgid "expected at least one argument"
msgstr "minst ett parametervärde förväntades"

#: ../../../../../../usr/lib64/python3.10/argparse.py:2171
#, python-format
msgid "expected %s argument"
msgid_plural "expected %s arguments"
msgstr[0] "förväntade sig %s parametervärde"
msgstr[1] "förväntade sig %s parametervärden"

#: ../../../../../../usr/lib64/python3.10/argparse.py:2229
#, python-format
msgid "ambiguous option: %(option)s could match %(matches)s"
msgstr "tvetydigt parameternamn: %(option)s skulle kunna matcha %(matches)s"

#: ../../../../../../usr/lib64/python3.10/argparse.py:2293
#, python-format
msgid "unexpected option string: %s"
msgstr "oväntad parameternamnssträng: %s"

#: ../../../../../../usr/lib64/python3.10/argparse.py:2490
#, python-format
msgid "%r is not callable"
msgstr "%r är inte anropsbar"

#: ../../../../../../usr/lib64/python3.10/argparse.py:2507
#, python-format
msgid "invalid %(type)s value: %(value)r"
msgstr "felaktigt %(type)s-värde: %(value)r"

#: ../../../../../../usr/lib64/python3.10/argparse.py:2518
#, python-format
msgid "invalid choice: %(value)r (choose from %(choices)s)"
msgstr "felaktigt val: %(value)r (välj från %(choices)s)"

#: ../../../../../../usr/lib64/python3.10/argparse.py:2594
#, python-format
msgid "%(prog)s: error: %(message)s\n"
msgstr "%(prog)s: fel: %(message)s\n"
